{
  "name": "webpack",
  "version": "1.0.9",
  "license": "MIT",
  "scripts": {
    "compile-template": "node ./scripts/replace-var.js",
    "build:dev": "rollup --config ./bundle-configs/rollup.config.development.ts",
    "server:dev": "ts-node-dev --transpile-only --project src/server/tsconfig.json ./src/server/index.ts",
    "start:dev": "npm-run-all -p server:dev \"build:dev --watch\"",
    "build:prod": "rollup --config ./bundle-configs/rollup.config.production.ts",
    "start": "ts-node --transpile-only --project src/server/tsconfig.json ./src/server/index.ts",
    "preversion": "echo \"Preversion script goes here (e.g. yarn run test)\" && exit 0",
    "postversion": "git push --tags && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/types": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.34.2",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node-dev": "^1.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.0.0"
  },
  "cdnDependecies": [
    "https://unpkg.com/react@17/umd/react.production.min.js",
    "https://unpkg.com/react-dom@17/umd/react-dom.production.min.js",
    "https://unpkg.com/lodash@4.17.20/lodash.min.js"
  ],
  "cdnDevDependecies": [
    "https://unpkg.com/react@17/umd/react.development.js",
    "https://unpkg.com/react-dom@17/umd/react-dom.development.js",
    "https://unpkg.com/lodash@4.17.20/lodash.js"
  ]
}
